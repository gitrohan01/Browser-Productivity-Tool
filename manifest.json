{
  "manifest_version": 3,
  "name": "YouTube Ad Bypass",
  "version": "1.1",
  "permissions": ["tabs", "scripting", "activeTab"],
  "host_permissions": ["*://*.youtube.com/*"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["content.js"]
    }
  ]
}
